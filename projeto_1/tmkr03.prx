// Substituido pelo assistente de conversao do AP5 IDE em 21/06/01 ==> Function C003Imp
Static Function C003Imp()

cPerg   :="TMK003"
cArqTrab:=""
nomeprog:="TMKR03"
cObs	  := "" 	
cVendAnt:= ""
cCliente:= ""
cFone   := ""
lFirst  :=.F.
cCpf	  :=""
cRg     :="" 
cLinha  := ""
nInd    := 0

//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
//Ё Variaveis utilizadas para Impressao do Cabecalho e Rodape    Ё
//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
cbtxt    := SPACE(10)
cbcont   := 0
li      	:= 80
m_pag    := 1

//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
//Ё Verifica se deve comprimir ou nao                            Ё
//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
nTipo := IIF(aReturn[4]==1,15,18)

cMoeda := STR0006 //"Valores em R$"
titulo := titulo+" "+cMoeda

SetRegua(RecCount())		// Total de Elementos da regua

If TYPE("uPorPrograma") == "U"

   // Chamado pelo Menu - Relatorio de Pedidos
	
	cArqTrab := CriaTrab("",.F.)
	IndRegua(cString,cArqTrab,"SUA->UA_FILIAL+SUA->UA_VEND+DTOS(SUA->UA_EMISSAO)",,,STR0007) //"Selecionando Registros..."
	dbCommit()	

	DbSelectArea("SUA")
	DbSeek(xFilial("SUA")+(mv_par01),.T.) //Vendedor
	
	While (!Eof()) .and. ((UA_Filial+UA_Vend >= xFilial("SUA")+mv_par01) .and. (UA_Filial+UA_Vend <= xFilial("SUA")+mv_par02))                   
		
		If lEnd
			@Prow()+1,001 PSAY STR0008 //"CANCELADO PELO OPERADOR"
			Exit
		EndIf
	
		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Verifica intervalo                                       Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		If (UA_EMISSAO > Mv_par04) .or. (UA_EMISSAO < Mv_par03)
		   DbSkip()
			Loop
	   Endif		
		

		If !Empty(Mv_Par05) .and. !Empty(Mv_Par06)
   		If UA_NUM < Mv_Par05 .or. UA_NUM > Mv_Par06
		      DbSkip()
			   Loop
	      Endif		
	   Endif		

		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Verifica Pedido                                          Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		If UA_OPER <> "P"
		   DbSkip()
			Loop
	   Endif		
		

		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Verifica Pedidos Cancelados                              Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		If UA_CANC == "S"
		   DbSkip()
			Loop
	   Endif		
	
		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Impressao do Cabecalho.                                  Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		If Li > 55
			cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
		EndIf

	   If lFirst
	      cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
		Endif	

		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Impressao Do Pedido                                      Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		DbSelectArea("SA3")
		If DbSeek(xFilial("SA3")+SUA->UA_VEND)
			cVendAnt := SA3->A3_NOME
		Endif
		
	   lFirst:=.t.
		incregua()
		DbSelectArea("SUA")	
		@ Li,004 PSAY xFilial("SUA") Picture "99"
		@ Li,014 PSAY UA_NUM 		  Picture "999999"
		@ Li,024 PSAY Alltrim(UA_Vend)+" - "+Left(cVendAnt,20)
	   @ li,056 PSAY UA_EMISSAO 	  Picture "99/99/99"
		@ li,076 PSAY UA_STATUS
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1

		cCond := ""
		DbSelectArea("SE4")
		DbSetOrder(1)
		If DbSeek(xFilial("SE4")+SUA->UA_CONDPG)
			cCond:=E4_CODIGO+" - "+E4_DESCRI
		Endif

		cCpf := ""
		cRg  := ""
		DbSelectArea("SA1")
		DbSetOrder(1)
		If DbSeek(xFilial("SA1")+SUA->UA_CLIENTE+SUA->UA_LOJA)
	   	cCpf:=A1_CGC 
			If !Empty(A1_INSCR)
	    		cRg := A1_INSCR
	   	Else
		   	cRg := A1_RG
			Endif			
		Endif
		
		@ Li,001 PSAY STR0009+cCond  //"Cond.Pagto: "
		@ Li,040 PSAY STR0010+A1_COD+" - "+LEFT(A1_NOME,20)//"Cliente: "
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1

	   @ Li,001 PSAY STR0011+cRg  //"Inscr.Estadual/RG: "
		@ Li,040 PSAY STR0012+cCpf //"CPF/CGC:"
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1	
	
		@ Li,001 PSAY OemToAnsi(STR0013)+SUA->UA_ENDCOB //"Endere┤o p/ Cobran┤a: "
		Li := Li+1
		@ Li,001 PSAY STR0014 //"Compl.: "
		@ Li,040 PSAY STR0015+SUA->UA_CEPC //"Cep: "
		Li := Li+1
		@ Li,001 PSAY STR0016+SUA->UA_BAIRROC //"Bairro: "
		@ Li,040 PSAY STR0017+SUA->UA_MUNC+"/"+SUA->UA_ESTC //"Cidade/UF: "
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1
		
		@ Li,001 PSAY OemToAnsi(STR0018)+SUA->UA_ENDENT //"Endere┤o p/ Entrega: "
		Li := Li+1
		@ Li,001 PSAY STR0014 //"Compl.: "
		@ Li,040 PSAY STR0015 +SUA->UA_CEPE //"Cep: "
		Li := Li+1
		@ Li,001 PSAY STR0016+SUA->UA_BAIRROE //"Bairro: "
		@ Li,040 PSAY STR0017+SUA->UA_MUNE+"/"+SUA->UA_ESTE //"Cidade/UF: "
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1
	
	   @ Li,001 PSAY STR0019 +A1_Tel //"TeleFone: "
	
		DbSelectArea("SA4")
		DbSetOrder(1)
		If DbSeek(xFilial("SA4")+SUA->UA_Transp)
			@ Li,040 PSAY STR0020  +A4_COD+" - "+LEFT(A4_NOME,20) //"Transp.: "
			Li := Li+2
		Else
		   Li := Li+1	
		Endif
		
		@ Li,001 PSAY STR0021 //"Obs:"
		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Imprime o campo memo Observacao.                             Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		If ! Empty(SUA->UA_CODOBS)
			cObs := MSMM(SUA->UA_CODOBS,43)
			nInd := 1
			While .T.
				cLinha := Substr(cObs,nInd,43)
				If ! Empty(cLinha)
					@ li,006 PSAY cLinha
					Li := Li+1
				Else
					Exit
				Endif	
				nInd := nInd + 43
			End
		Endif
		Li := Li+1	
		@ Li,000 PSAY Repl("-",80)
		Li := Li+1
		
   	@ Li,001 PSAY OemtoAnsi(STR0022) //"Codigo          Descri┤└o              Un     Qtde.     Vlr. Unit.        Total" 
		Li := Li+1
		@ Li,000 PSAY Repl("-",80)
		Li := Li+1	

		DbSelectArea("SUB")
		DbSetOrder(1)
		DbSeek(xFilial("SUB")+SUA->UA_NUM)
		While (.not. Eof()) .and. (xFilial("SUB")+SUA->UA_NUM == SUB->UB_FILIAL+SUB->UB_NUM)

         DbSelectArea("SB1")
         DbSetOrder(1)
         If DbSeek(xFilial("SB1")+SUB->UB_PRODUTO) 
         	@ Li,001 PSAY B1_Cod
         	@ Li,017 PSAY Left(B1_DESC,20)
         	@ Li,040 PSAY B1_Um
			Endif
				
         @ Li,043 PSAY SUB->UB_QUANT   Picture "@Z 999999,99"
         @ Li,053 PSAY SUB->UB_VRUNIT  Picture "@Z 99,999,999.99"
         @ Li,067 PSAY SUB->UB_VLRITEM Picture "@Z 99,999,999.99"			
			
         Li := Li+1
         If Li > 55
            cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
         EndIf

         DbSelectArea("SUB")
         DbSkip()
		End

	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1	
		@ Li,000 PSAY Padc(STR0023,80) //"Forma de Pagamento"
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1	
		@ Li,001 PSAY STR0024 //"Data"
		@ Li,022 PSAY STR0025 //"Valor"
		@ Li,038 PSAY STR0026 //"Forma" 
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		
	   DbSelectArea("SE1")
		DbSetOrder(1)
		DbSeek(xFilial("SE1")+SUA->UA_SERIE+SUA->UA_DOC)
		While (.not. Eof()) .and. (xFilial("SE1")+E1_NUM == SUA->UA_FILIAL+SUA->UA_DOC)
         Li := Li+1
         If Li > 55
            cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
         EndIf

         @ Li,001 PSAY E1_VENCTO
         @ Li,014 PSAY E1_VALOR Picture "@Z 99,999,999.99"
         @ Li,040 PSAY E1_TIPO  Picture "@!"
			DbSelectarea("SE1")
         DbSkip()
	   End			

		DbSelectArea("SUA")
		DbSkip()

	End	
Else

	DbSelectArea("SUA")
 	DbSetOrder(1) 
	If DbSeek(xFilial("SUA")+(PARAMIXB)) //Pedido
	
		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Impressao do Cabecalho.                                  Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		If Li > 55
			cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
		EndIf

	   If lFirst
			cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
		Endif	

		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Impressao Do Pedido                                      Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		DbSelectArea("SA3")
		DbSeek(xFilial("SA3")+SUA->UA_VEND)
		cVendAnt := SA3->A3_NOME

	   lFirst:=.t.
		DbSelectArea("SUA")	

		@ Li,004 PSAY xFilial("SUA") Picture "99"
		@ Li,014 PSAY UA_NUM 		  Picture "999999"
		@ Li,024 PSAY Alltrim(UA_VEND)+" - "+Left(cVendAnt,20)
	   @ li,056 PSAY UA_EMISSAO 	  Picture "99/99/99"
		@ li,076 PSAY UA_STATUS
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1

		cCond := ""
		DbSelectArea("SE4")
		DbSetOrder(1)
		If DbSeek(xFilial("SE4")+SUA->UA_CONDPG)
			cCond:=E4_CODIGO+" - "+E4_DESCRI
		Endif	

		cCpf := ""
		cRg  := ""
		DbSelectArea("SA1")
		DbSetOrder(1)
		If DbSeek(xFilial("SA1")+SUA->UA_CLIENTE+SUA->UA_LOJA)
	   	cCpf:=A1_CGC 
			If !Empty(A1_INSCR)
	    		cRg :=A1_INSCR
	   	Else
		   	cRg :=A1_RG
			Endif
		Endif	

		@ Li,001 PSAY STR0009+cCond  //"Cond.Pagto: "
		@ Li,040 PSAY STR0010+A1_COD+" - "+LEFT(A1_NOME,20) //"Cliente: "
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1

	   @ Li,001 PSAY STR0011+cRg 	//"Inscr.Estadual/RG: "
		@ Li,040 PSAY STR0012+cCpf	//"CPF/CGC:"
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1	
	
		@ Li,001 PSAY OemToAnsi(STR0013)+SUA->UA_ENDCOB //"Endere┤o p/ Cobran┤a: "
		Li := Li+1
		@ Li,001 PSAY STR0014 //"Compl.: "
		@ Li,040 PSAY STR0015+SUA->UA_CEPC //"Cep: "
		Li := Li+1
		@ Li,001 PSAY STR0016+SUA->UA_BAIRROC //"Bairro: "
		@ Li,040 PSAY STR0017+SUA->UA_MUNC+"/"+SUA->UA_ESTC //"Cidade/UF: "
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1
		
		@ Li,001 PSAY OemToAnsi(STR0018)+SUA->UA_ENDENT //"Endere┤o p/ Entrega: "
		Li := Li+1
		@ Li,001 PSAY STR0014 //"Compl.: "
		@ Li,040 PSAY STR0015+SUA->UA_CEPE //"Cep: "
		Li := Li+1
		@ Li,001 PSAY STR0016+SUA->UA_BAIRROE //"Bairro: "
		@ Li,040 PSAY STR0017+SUA->UA_MUNE+"/"+SUA->UA_ESTE //"Cidade/UF: "
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1
	
	   @ Li,001 PSAY STR0019 +A1_TEL //"TeleFone: "

		DbSelectArea("SA4")
		DbSetOrder(1)
		If DbSeek(xFilial("SA4")+SUA->UA_TRANSP)
			@ Li,040 PSAY STR0020 +A4_COD+" - "+LEFT(A4_NOME,20) //"Transp.: "
			Li := Li+2	
		Else
		   Li := Li+1	
		Endif

		@ Li,001 PSAY STR0021 //"Obs:"
		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Imprime o campo memo Observacao.                             Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		If ! Empty(SUA->UA_CODOBS)
			cObs := MSMM(SUA->UA_CODOBS,43)
			nInd := 1
			While .T.
				cLinha := Substr(cObs,nInd,43)
				If ! Empty(cLinha)
					@ li,006 PSAY cLinha
					Li := Li+1
				Else
					Exit
				Endif	
				nInd := nInd + 43
			End
		Endif
		Li := Li+1		
		@ Li,000 PSAY Repl("-",80)
		Li := Li+1
		
   	@ Li,001 PSAY OemtoAnsi(STR0022) //"Codigo          Descri┤└o              Un     Qtde.     Vlr. Unit.        Total" 
		Li := Li+1
		@ Li,000 PSAY Repl("-",80)
		Li := Li+1	

		DbSelectArea("SUB")
		DbSetOrder(1)
		DbSeek(xFilial("SUB")+SUA->UA_NUM)
		While (.not. Eof()) .and. (xFilial("SUB")+SUA->UA_NUM == SUB->UB_FILIAL+SUB->UB_NUM)

         DbSelectArea("SB1")
         DbSetOrder(1)
         If DbSeek(xFilial("SB1")+SUB->UB_PRODUTO) 
         	@ Li,001 PSAY B1_COD
         	@ Li,017 PSAY Left(B1_DESC,20)
         	@ Li,040 PSAY B1_UM
			Endif
				
         @ Li,043 PSAY SUB->UB_QUANT   Picture "@Z 999999,99"
         @ Li,053 PSAY SUB->UB_VRUNIT  Picture "@Z 99,999,999.99"
         @ Li,067 PSAY SUB->UB_VLRITEM Picture "@Z 99,999,999.99"			

         DbSelectArea("SUB")
         DbSkip()

         Li := Li+1
         If Li > 55
            cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
         EndIf
		End

	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1	
		@ Li,000 PSAY Padc(STR0023,80) //"Forma de Pagamento"
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)
		Li := Li+1	
		@ Li,001 PSAY STR0024 //"Data"
		@ Li,022 PSAY STR0025 //"Valor"
		@ Li,038 PSAY STR0026 //"Forma" 
		Li := Li+1
	   @ Li,000 PSAY Repl("-",80)

		
	   DbSelectArea("SE1")
		DbSetOrder(1)
		DbSeek(xFilial("SE1")+SUA->UA_SERIE+SUA->UA_DOC)
		While (.not. Eof()) .and. (xFilial("SE1")+E1_NUM == SUA->UA_FILIAL+SUA->UA_DOC)
         Li := Li+1
         If Li > 55
            cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
         EndIf

         @ Li,001 PSAY E1_VENCTO
         @ Li,014 PSAY E1_VALOR Picture "@Z 99,999,999.99"
         @ Li,040 PSAY E1_TIPO  Picture "@!"
			DbSelectarea("SE1")
         DbSkip()
	   End			

	Endif
Endif	

If !lfirst
  cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
  @ Li+1,001 PSAY OemToAnsi(STR0027) //"Nao ha informacoes para imprimir este relatorio"
Endif

roda(cbcont,cbtxt,tamanho)

DbSelectArea("SUA")
RetIndex("SUA")
Set Filter To
DbSetorder(1)

If TYPE("uPorPrograma") == "U"
   fErase(cArqTrab+OrdBagExt())
	fErase(cArqTrab)
Endif	

Set Device To Screen

If aReturn[5] == 1
	Set Printer TO 
	dbcommitAll()
	ourspool(wnrel)
EndIf

MS_FLUSH()

Return .T.
